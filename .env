# Anthropic API Key
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google OAuth2 Client ID and Secret for Gmail
GMAIL_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
GMAIL_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET
GMAIL_REDIRECT_URI=YOUR_GMAIL_REDIRECT_URI
# Outlook OAuth2 Client ID, Secret, and Tenant ID
OUTLOOK_CLIENT_ID=YOUR_OUTLOOK_CLIENT_ID
OUTLOOK_CLIENT_SECRET=YOUR_OUTLOOK_CLIENT_SECRET
OUTLOOK_TENANT_ID=YOUR_TENANT_ID
OUTLOOK_REDIRECT_URI=YOUR_OUTLOOK_REDIRECT_URI
# Redis connection settings (optional, can be configured in the code as well)
REDIS_HOST=localhost
REDIS_PORT=6379
